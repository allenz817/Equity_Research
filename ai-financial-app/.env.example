# Google Gemini API Key (required for AI extraction)
GEMINI_API_KEY=your-gemini-api-key

# Google Cloud Project ID (optional, for Vertex AI)
GOOGLE_CLOUD_PROJECT_ID=your-project-id

# Path to Google Cloud service account credentials JSON file (optional, for Vertex AI)
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/credentials.json

# Port for the web server (defaults to 3000)
PORT=3000

# Node environment
NODE_ENV=development
